#include <iostream>
using namespace std;

class Mother{
 public:
    Mother(){}
    void display(){
        cout<<"Why are you so gay."<<endl;
    }
};

class Daughter : public Mother{
public:
    Daughter(){}
    
    void display(){
        cout<<" Gay af, shut up bitch."<<endl;
    }
};

int main()
{
    Daughter d;
    d.display();
    return 0;
}
